generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id         Int     @id @default(autoincrement())
  phone      String  @unique
  password   String  @db.VarChar(255)
  nickname   String? @db.VarChar(64)
  name       String? @db.VarChar(128)
  avatar_url String?
}
